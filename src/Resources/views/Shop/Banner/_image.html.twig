{% if banner.url is not null %}
    <a href="{{ banner.url }}" target="_blank">
        <img src="{{ banner.imageName|imagine_filter('odiseo_sylius_banner_plugin_image') }}" alt="{{ banner.imageName }}">
    </a>
{% else %}
    <img src="{{ banner.imageName|imagine_filter('odiseo_sylius_banner_plugin_image') }}" alt="{{ banner.imageName }}">
{% endif %}
